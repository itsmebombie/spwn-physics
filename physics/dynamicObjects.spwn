extract obj_props
f1 = import "staticObject.spwn"
let objects = []

add = (objInfo) {
    x1 = 60; x2 = 00; x3 = 00; x4 = 30; x5 = 90;
    y1 = -90; y2 = -60; y3 = -120; y4 = -90; y5 = -90;

    group1 = ?g
    group2 = ?g
    $.add(obj { //add spawn trigger
        OBJ_ID: 1268,//spawn trigger id
        X: x4,
        Y: y4,
        TARGET: group1,
    })
    $.add(obj { //add stop trigger
        OBJ_ID: 1616,//stop trigger id
        X: x5,
        Y: y5,
        GROUPS: group2,
        TARGET: group1,
        SPAWN_TRIGGERED: true,
        MULTI_TRIGGER: true,
    })
    $.add(obj { //add move trigger
        OBJ_ID: 901,//move trigger id
        X: x1,
        Y: y1,
        DURATION: 500 + (1/$.exp(objInfo[2]/10))*2000,
        MOVE_Y: -300000,
        GROUPS: group1,
        TARGET: objInfo[0],
        SPAWN_TRIGGERED: true,
        MULTI_TRIGGER: true,
    })
    $.add(obj { //add collision1 trigger
        OBJ_ID: 1815,//collision trigger id
        X: x2,
        Y: y2,
        BLOCK_A: objInfo[1],
        BLOCK_B: f1(1b),
        TARGET: group2,
        ACTIVATE_GROUP: true,
    })
    $.add(obj { //add collision2 trigger
        OBJ_ID: 1815,//collision trigger id
        X: x3,
        Y: y3,
        BLOCK_A: objInfo[1],
        BLOCK_B: f1(1b),
        TARGET: group1,
        ACTIVATE_GROUP: true,
        ACTIVATE_ON_EXIT: true,
    })
}
function = (objectGroup = 0g, objectCollisionGroup = 0b, weight = 2) {
    if objectGroup == 0g {
        return objects 
    } else if objectCollisionGroup == 0b {
        return objects 
    } else {
        objects.push([objectGroup, objectCollisionGroup, weight])
        add([objectGroup, objectCollisionGroup, weight])
        return objects 
    }
}

return function
